[run]
source = dcolumn

omit =
    ../*migrations*
    *urls*
    *tests*
    *admin*

[report]
show_missing = True
skip_covered = True
precision = 2

# Regexes for lines to exclude from consideration
exclude_lines =
    # Have to re-enable the standard pragma
    pragma: no cover

    # Don't complain about missing debug-only code:
    if log\.is*

[html]
title = Django Dynamic Column Coverage Report
directory = docs/htmlcov
